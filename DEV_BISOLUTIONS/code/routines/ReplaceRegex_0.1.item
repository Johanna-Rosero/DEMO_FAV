package routines;


public class ReplaceRegex {

    public static String cleanColumn(String inputColumn) {
        if (inputColumn != null) {
        	
            return StringHandling.CHANGE(inputColumn,"\\n", "");
        } else {
        	return null;
        }
        
    }
    public static String cleanQuote(String inputColumn) {
        if (inputColumn != null) {
        	
        	String result = inputColumn.replace("\"", "");
            return result;
        } else {
        	return null;
        }
        
    }
}

    <elementParameter field="GUESS_SCHEMA" name="GUESS_SCHEMA" value="&quot;&quot;"/>
    <elementParameter field="MEMO_SQL" name="QUERY" value="&quot;SELECT &#xD;&#xA;TMP.CODIGOJDEPROVEEDOR,&#xD;&#xA;TMP.NUMERODOCUMENTOPROVEEDOR,&#xD;&#xA;TMP.NOMBREPROVEEDOR,&#xD;&#xA;CASE WHEN TMP.TELUSUARIOPEDIDO IS NULL THEN TMP.TELCONTACTOB2B ELSE TMP.TELUSUARIOPEDIDO END AS TELEFONO,&#xD;&#xA;CASE WHEN TMP.EMAILUSUARIOPEDIDO IS NULL THEN TMP. MAILCONTACTOB2B ELSE TMP.EMAILUSUARIOPEDIDO END AS CORREO&#xD;&#xA;FROM&#xD;&#xA;(SELECT&#xD;&#xA;P.CODIGOJDEPROVEEDOR,&#xD;&#xA;P.NUMERODOCUMENTOPROVEEDOR,&#xD;&#xA;P.NOMBREPROVEEDOR,&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;(SELECT&#xD;&#xA;DCR.NUMTELPRINCIPAL AS TELCONTACTOPRI&#xD;&#xA;FROM SMXCORP.SSPCOTCONPERLOCREL CR&#xD;&#xA;INNER JOIN SMXCORP.SSPCOTDATCONPERLOC DCR&#xD;&#xA;ON CR.CODIGOCOMPANIA = DCR.CODIGOCOMPANIA  AND CR.SECUENCIALCONTACTO = DCR.SECUENCIALCONTACTO&#xD;&#xA;WHERE CR.ESTADO = 'ACT' AND DCR.ESTADO = 'ACT' AND DCR.CODIGOTIPOCONTACTO = 'B2B'&#xD;&#xA;AND CR.CODIGOENTIDAD = P.CODIGOENTIDADPROVEEDOR AND CR.VALORTIPOENTIDAD =  P.VALORTIPOENTIDADPROVEEDOR FETCH FIRST 1 ROWS ONLY) AS TELCONTACTOB2B,&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;(SELECT&#xD;&#xA;DCR.EMAILPRINCIPAL AS MAILCONTACTOPRI&#xD;&#xA;FROM SMXCORP.SSPCOTCONPERLOCREL CR&#xD;&#xA;INNER JOIN SMXCORP.SSPCOTDATCONPERLOC DCR&#xD;&#xA;ON CR.CODIGOCOMPANIA = DCR.CODIGOCOMPANIA  AND CR.SECUENCIALCONTACTO = DCR.SECUENCIALCONTACTO&#xD;&#xA;WHERE CR.ESTADO = 'ACT' AND DCR.ESTADO = 'ACT' AND DCR.CODIGOTIPOCONTACTO = 'B2B'&#xD;&#xA;AND CR.CODIGOENTIDAD = P.CODIGOENTIDADPROVEEDOR AND CR.VALORTIPOENTIDAD =  P.VALORTIPOENTIDADPROVEEDOR FETCH FIRST 1 ROWS ONLY) AS MAILCONTACTOB2B,&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;(SELECT&#xD;&#xA;U.EMAILUSUARIO&#xD;&#xA;FROM SMXCORP.SSPCOVUSUFUNPERLOC U&#xD;&#xA;INNER JOIN SMXMBASE.KSSEGTROLUSE RU&#xD;&#xA;ON U.CODIGOUSUARIO = RU.USERID&#xD;&#xA;INNER JOIN SMXCORP.SSPCOTFUNCIONARIO F&#xD;&#xA;ON F.CODIGOCOMPANIA = U.CODIGOCOMPANIA AND F.CODIGOFUNCIONARIO = U.CODIGOFUNCIONARIO&#xD;&#xA;LEFT JOIN SMXCORP.SSPCOTPERSONA PER&#xD;&#xA;ON PER.CODIGOPERSONA = F.CODIGOPERSONA&#xD;&#xA;WHERE U.CODIGOENTIDAD = P.CODIGOENTIDADPROVEEDOR&#xD;&#xA;AND U.VALORTIPOENTIDAD =  P.VALORTIPOENTIDADPROVEEDOR AND SUBSTR(TRIM(U.CODIGOUSUARIO),1,9) &lt;> 'B2BPROADM'&#xD;&#xA;AND RU.ROLEID = 'B2BPED' AND RU.SYSID = 'B2B' AND U.ESTADOUSUARIOPERSONALOCALIZACION = 'ACT' FETCH FIRST 1 ROWS ONLY) AS EMAILUSUARIOPEDIDO,&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;(SELECT&#xD;&#xA;PER.TELEFONODOMICILIO&#xD;&#xA;FROM SMXCORP.SSPCOVUSUFUNPERLOC U&#xD;&#xA;INNER JOIN SMXMBASE.KSSEGTROLUSE RU&#xD;&#xA;ON U.CODIGOUSUARIO = RU.USERID&#xD;&#xA;INNER JOIN SMXCORP.SSPCOTFUNCIONARIO F&#xD;&#xA;ON F.CODIGOCOMPANIA = U.CODIGOCOMPANIA AND F.CODIGOFUNCIONARIO = U.CODIGOFUNCIONARIO&#xD;&#xA;LEFT JOIN SMXCORP.SSPCOTPERSONA PER&#xD;&#xA;ON PER.CODIGOPERSONA = F.CODIGOPERSONA&#xD;&#xA;WHERE U.CODIGOENTIDAD = P.CODIGOENTIDADPROVEEDOR&#xD;&#xA;AND U.VALORTIPOENTIDAD = P.VALORTIPOENTIDADPROVEEDOR AND SUBSTR(TRIM(U.CODIGOUSUARIO),1,9) &lt;> 'B2BPROADM'&#xD;&#xA;AND RU.ROLEID = 'B2BPED' AND RU.SYSID = 'B2B' AND U.ESTADOUSUARIOPERSONALOCALIZACION = 'ACT' FETCH FIRST 1 ROWS ONLY) AS TELUSUARIOPEDIDO&#xD;&#xA;FROM SMXSIC.SCSPEVPROVEEDOR P&#xD;&#xA;WHERE P.CODIGOCOMPANIA = 1 &#xD;&#xA;AND P.ESTADOPROVEEDOR = 1&#xD;&#xA;AND CAST(P.CODIGOPROVEEDOR AS INTEGER) = P.CODIGOJDEPROVEEDOR &#xD;&#xA;AND ((P.CODIGOPERSONA IS NULL AND P.CODIGOLOCALIZACIONPROVEEDOR IS NOT NULL) OR (P.CODIGOPERSONA IS NOT NULL AND P.CODIGOLOCALIZACIONPROVEEDOR IS NULL))&#xD;&#xA;) AS TMP&quot;"/>

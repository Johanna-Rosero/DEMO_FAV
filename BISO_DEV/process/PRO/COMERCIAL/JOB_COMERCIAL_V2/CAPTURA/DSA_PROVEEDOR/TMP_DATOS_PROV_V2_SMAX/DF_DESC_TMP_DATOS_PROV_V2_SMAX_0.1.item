    <elementParameter field="GUESS_SCHEMA" name="GUESS_SCHEMA" value="&quot;&quot;"/>
    <elementParameter field="MEMO_SQL" name="QUERY" value="&quot;SELECT&#xD;&#xA;   CAST(TMP.CODIGOPROVEEDOR AS INT) AS CODIGOPROVEEDOR,&#xD;&#xA; TMP.CODIGOJDEPROVEEDOR,&#xD;&#xA; &#xD;&#xA; TMP.NUMERODOCUMENTOPROVEEDOR,&#xD;&#xA; TMP.NOMBREPROVEEDOR,&#xD;&#xA; CASE &#xD;&#xA;     WHEN (TMP.NOMBRECONTACTOB2B IS NOT NULL) AND (TMP.NOMBRECONTACTOB2B &lt;> 'JARA BOLANOS MARIANITA DE LOS ANGELES') THEN TMP.NOMBRECONTACTOB2B &#xD;&#xA;  WHEN (TMP.NOMBRECONTACTOPED IS NOT NULL) AND (TMP.NOMBRECONTACTOPED &lt;> 'JARA BOLANOS MARIANITA DE LOS ANGELES') THEN TMP.NOMBRECONTACTOPED || ' *'&#xD;&#xA; ELSE TMP.NOMBRECONTACTOADM &#xD;&#xA; END AS NOMBRECONTACTO,&#xD;&#xA; &#xD;&#xA; CASE WHEN TMP.TELCONTACTOB2B IS NOT NULL THEN TMP.TELCONTACTOB2B ELSE TMP.TELCONTACTOPRI END AS TELEFONO,&#xD;&#xA; &#xD;&#xA; CASE &#xD;&#xA;     WHEN (TMP.MAILCONTACTOB2B IS NOT NULL) AND (TMP.MAILCONTACTOB2B &lt;> 'mjara@supermaxi.com') AND (TMP.NOMBRECONTACTOB2B IS NOT NULL) AND (TMP.NOMBRECONTACTOB2B &lt;> 'JARA BOLANOS MARIANITA DE LOS ANGELES') THEN TMP.MAILCONTACTOB2B &#xD;&#xA;  WHEN (TMP.MAILCONTACTOPED IS NOT NULL) AND (TMP.MAILCONTACTOPED &lt;> 'mjara@supermaxi.com') AND (TMP.NOMBRECONTACTOPED IS NOT NULL) AND (TMP.NOMBRECONTACTOPED &lt;> 'JARA BOLANOS MARIANITA DE LOS ANGELES') THEN TMP.MAILCONTACTOPED&#xD;&#xA;     WHEN (TMP.MAILCONTACTOB2B IS NOT NULL) AND (TMP.MAILCONTACTOB2B &lt;> 'mjara@supermaxi.com') THEN TMP.MAILCONTACTOB2B &#xD;&#xA;  WHEN (TMP.MAILCONTACTOPED IS NOT NULL) AND (TMP.MAILCONTACTOPED &lt;> 'mjara@supermaxi.com') THEN TMP.MAILCONTACTOPED&#xD;&#xA;  WHEN (TMP.MAILCONTACTOPRI IS NOT NULL) AND (TMP.MAILCONTACTOPRI &lt;> 'mjara@supermaxi.com') THEN TMP.MAILCONTACTOPRI&#xD;&#xA; ELSE '*' &#xD;&#xA; END AS EMAIL,&#xD;&#xA; &#xD;&#xA; TMP.DIRECCIONPRINCIPAL,&#xD;&#xA; &#xD;&#xA; TMP.CANTON AS CANTON,&#xD;&#xA; &#xD;&#xA; TMP.PROVINCIA AS PROVINCIA,&#xD;&#xA; &#xD;&#xA; '' AS GERENTECOMERCIAL,&#xD;&#xA; &#xD;&#xA; TMP.SUBBODEGA,&#xD;&#xA; TMP.RAZONSOCIALPROVEEDOR,&#xD;&#xA; TMP.FECHACREACION,&#xD;&#xA; TMP.ESTADOPROVEEDOR,&#xD;&#xA; TMP.ABR_TIPO_EMPRESA,&#xD;&#xA; TMP.NOM_TIPO_EMPRESA,&#xD;&#xA; TMP.ABR_CALIFICACION_EMPRESA,&#xD;&#xA; TMP.NOM_CALIFICACION_EMPRESA,&#xD;&#xA; TMP.ESARTESANO,&#xD;&#xA; TMP.ORIGENPROVEEDOR,&#xD;&#xA; TMP.ESIMPORTADOR,&#xD;&#xA; TMP.PLAZO_PAGO,&#xD;&#xA; TMP.VALOR_PLAZO_PAGO,&#xD;&#xA; TMP.CIUDAD,&#xD;&#xA; TMP.ANALITIX&#xD;&#xA; &#xD;&#xA;FROM&#xD;&#xA;(SELECT&#xD;&#xA;&#xD;&#xA; P.CODIGOPROVEEDOR,&#xD;&#xA; P.CODIGOJDEPROVEEDOR,&#xD;&#xA; &#xD;&#xA; &#xD;&#xA; P.NUMERODOCUMENTOPROVEEDOR,&#xD;&#xA; &#xD;&#xA; P.NOMBREPROVEEDOR,&#xD;&#xA; (select &#xD;&#xA;  t.codigoreferencia || ' - ' || t.NOMBREAREATRABAJO&#xD;&#xA; from SMXSIC.SCSPETPROCLA pc&#xD;&#xA; inner join SCSPETCLASIFICACION c&#xD;&#xA; on pc.codigocompania = c.codigocompania and pc.codigoclasificacion = c.codigoclasificacion&#xD;&#xA; inner join smxsic.SCSPETBODEGA sb&#xD;&#xA; on c.codigocompania = sb.codigocompania and c.codigobodega = sb.codigobodega&#xD;&#xA; inner join SSPCOTAREATRABAJO t&#xD;&#xA; on sb.CODIGOAREATRABAJO=t.CODIGOAREATRABAJO and sb.CODIGOCOMPANIA=t.CODIGOCOMPANIA&#xD;&#xA; where pc.codigoCompania=1 &#xD;&#xA; and pc.estadoproveedorclasificacion = '1'&#xD;&#xA; and c.estadoclasificacion = '1'&#xD;&#xA; and sb.estadobodega = '1'&#xD;&#xA; and t.estadoareatrabajo = 'ACT'&#xD;&#xA; and pc.codigoproveedor = P.CODIGOPROVEEDOR FETCH FIRST 1 ROWS ONLY) AS SUBBODEGA,&#xD;&#xA; &#xD;&#xA; &#xD;&#xA; (SELECT&#xD;&#xA;  PRO.NOMBREDIVGEOPOL&#xD;&#xA; FROM smxcorp.SSPCOTDATCONPERLOC DCR&#xD;&#xA; INNER JOIN SSPCOTDIVISIONGEOPOLITICA CAN ON DCR.CODCIUDIRPRI = CAN.CODIGODIVGEOPOL&#xD;&#xA; INNER JOIN SSPCOTDIVISIONGEOPOLITICA PRO ON PRO.CODIGODIVGEOPOL = CAN.CODIGODIVGEOPOLPADRE&#xD;&#xA; WHERE DCR.ESTADO = 'ACT' AND DCR.CODIGOTIPOCONTACTO = 'PRI' AND CAN.ESTADO = 'ACT' &#xD;&#xA; AND DCR.CODIGOENTIDAD = P.CODIGOENTIDADPROVEEDOR AND DCR.VALORTIPOENTIDAD =  P.VALORTIPOENTIDADPROVEEDOR FETCH FIRST 1 ROWS ONLY) AS PROVINCIA,&#xD;&#xA;&#xD;&#xA; (SELECT&#xD;&#xA;  CAN.NOMBREDIVGEOPOL&#xD;&#xA; FROM smxcorp.SSPCOTDATCONPERLOC DCR&#xD;&#xA; INNER JOIN SSPCOTDIVISIONGEOPOLITICA CAN&#xD;&#xA; ON DCR.CODCIUDIRPRI = CAN.CODIGODIVGEOPOL&#xD;&#xA; WHERE DCR.ESTADO = 'ACT' AND DCR.CODIGOTIPOCONTACTO = 'PRI' AND CAN.ESTADO = 'ACT' &#xD;&#xA; AND DCR.CODIGOENTIDAD = P.CODIGOENTIDADPROVEEDOR AND DCR.VALORTIPOENTIDAD =  P.VALORTIPOENTIDADPROVEEDOR FETCH FIRST 1 ROWS ONLY) AS CANTON,&#xD;&#xA; &#xD;&#xA; (SELECT&#xD;&#xA;  DCR.DIRECCIONPRINCIPAL&#xD;&#xA; FROM smxcorp.SSPCOTDATCONPERLOC DCR&#xD;&#xA; WHERE DCR.ESTADO = 'ACT' AND DCR.CODIGOTIPOCONTACTO = 'PRI'&#xD;&#xA; AND DCR.CODIGOENTIDAD = P.CODIGOENTIDADPROVEEDOR AND DCR.VALORTIPOENTIDAD =  P.VALORTIPOENTIDADPROVEEDOR FETCH FIRST 1 ROWS ONLY) AS DIRECCIONPRINCIPAL,&#xD;&#xA; &#xD;&#xA; (SELECT&#xD;&#xA;  DCR.EMAILPRINCIPAL&#xD;&#xA; FROM smxcorp.SSPCOTDATCONPERLOC DCR&#xD;&#xA; WHERE DCR.ESTADO = 'ACT' AND DCR.CODIGOTIPOCONTACTO = 'PRI'&#xD;&#xA; AND DCR.CODIGOENTIDAD = P.CODIGOENTIDADPROVEEDOR AND DCR.VALORTIPOENTIDAD =  P.VALORTIPOENTIDADPROVEEDOR FETCH FIRST 1 ROWS ONLY) AS MAILCONTACTOPRI,&#xD;&#xA; &#xD;&#xA; (SELECT&#xD;&#xA;  DCR.NUMTELPRINCIPAL&#xD;&#xA; FROM smxcorp.SSPCOTDATCONPERLOC DCR&#xD;&#xA; WHERE DCR.ESTADO = 'ACT' AND DCR.CODIGOTIPOCONTACTO = 'PRI'&#xD;&#xA; AND DCR.CODIGOENTIDAD = P.CODIGOENTIDADPROVEEDOR AND DCR.VALORTIPOENTIDAD =  P.VALORTIPOENTIDADPROVEEDOR FETCH FIRST 1 ROWS ONLY) AS TELCONTACTOPRI,&#xD;&#xA; (SELECT&#xD;&#xA;  DCR.NUMTELCELULAR&#xD;&#xA; FROM smxcorp.SSPCOTDATCONPERLOC DCR&#xD;&#xA; WHERE DCR.ESTADO = 'ACT' AND DCR.CODIGOTIPOCONTACTO = 'PRI'&#xD;&#xA; AND DCR.CODIGOENTIDAD = P.CODIGOENTIDADPROVEEDOR AND DCR.VALORTIPOENTIDAD =  P.VALORTIPOENTIDADPROVEEDOR FETCH FIRST 1 ROWS ONLY) AS CELCONTACTOPRI,&#xD;&#xA;&#xD;&#xA; (SELECT&#xD;&#xA;  PER.NOMBRECOMPLETO&#xD;&#xA; FROM smxcorp.SSPCOTCONPERLOCREL CR&#xD;&#xA; INNER JOIN smxcorp.SSPCOTDATCONPERLOC DCR&#xD;&#xA; ON CR.CODIGOCOMPANIA = DCR.CODIGOCOMPANIA  AND CR.SECUENCIALCONTACTO = DCR.SECUENCIALCONTACTO&#xD;&#xA; INNER JOIN smxcorp.SSPCOTPERSONA PER&#xD;&#xA; ON DCR.CODIGOPERSONA = PER.CODIGOPERSONA&#xD;&#xA; WHERE CR.ESTADO = 'ACT' AND DCR.ESTADO = 'ACT' AND DCR.CODIGOTIPOCONTACTO = 'B2B' AND PER.ESTADOPERSONA = 'ACT'&#xD;&#xA; AND CR.CODIGOENTIDAD = P.CODIGOENTIDADPROVEEDOR AND CR.VALORTIPOENTIDAD =  P.VALORTIPOENTIDADPROVEEDOR FETCH FIRST 1 ROWS ONLY) AS NOMBRECONTACTOB2B,&#xD;&#xA; (SELECT&#xD;&#xA;  DCR.EMAILPRINCIPAL&#xD;&#xA; FROM smxcorp.SSPCOTCONPERLOCREL CR&#xD;&#xA; INNER JOIN smxcorp.SSPCOTDATCONPERLOC DCR&#xD;&#xA; ON CR.CODIGOCOMPANIA = DCR.CODIGOCOMPANIA  AND CR.SECUENCIALCONTACTO = DCR.SECUENCIALCONTACTO&#xD;&#xA; WHERE CR.ESTADO = 'ACT' AND DCR.ESTADO = 'ACT' AND DCR.CODIGOTIPOCONTACTO = 'B2B'&#xD;&#xA; AND CR.CODIGOENTIDAD = P.CODIGOENTIDADPROVEEDOR AND CR.VALORTIPOENTIDAD =  P.VALORTIPOENTIDADPROVEEDOR FETCH FIRST 1 ROWS ONLY) AS MAILCONTACTOB2B,&#xD;&#xA; (SELECT&#xD;&#xA;  DCR.NUMTELPRINCIPAL&#xD;&#xA; FROM smxcorp.SSPCOTCONPERLOCREL CR&#xD;&#xA; INNER JOIN smxcorp.SSPCOTDATCONPERLOC DCR&#xD;&#xA; ON CR.CODIGOCOMPANIA = DCR.CODIGOCOMPANIA  AND CR.SECUENCIALCONTACTO = DCR.SECUENCIALCONTACTO&#xD;&#xA; WHERE CR.ESTADO = 'ACT' AND DCR.ESTADO = 'ACT' AND DCR.CODIGOTIPOCONTACTO = 'B2B'&#xD;&#xA; AND CR.CODIGOENTIDAD = P.CODIGOENTIDADPROVEEDOR AND CR.VALORTIPOENTIDAD =  P.VALORTIPOENTIDADPROVEEDOR FETCH FIRST 1 ROWS ONLY) AS TELCONTACTOB2B,&#xD;&#xA; &#xD;&#xA; (SELECT&#xD;&#xA;  DCR.NUMTELCELULAR&#xD;&#xA; FROM smxcorp.SSPCOTCONPERLOCREL CR&#xD;&#xA; INNER JOIN smxcorp.SSPCOTDATCONPERLOC DCR&#xD;&#xA; ON CR.CODIGOCOMPANIA = DCR.CODIGOCOMPANIA  AND CR.SECUENCIALCONTACTO = DCR.SECUENCIALCONTACTO&#xD;&#xA; WHERE CR.ESTADO = 'ACT' AND DCR.ESTADO = 'ACT' AND DCR.CODIGOTIPOCONTACTO = 'B2B'&#xD;&#xA; AND CR.CODIGOENTIDAD = P.CODIGOENTIDADPROVEEDOR AND CR.VALORTIPOENTIDAD =  P.VALORTIPOENTIDADPROVEEDOR FETCH FIRST 1 ROWS ONLY) AS CELCONTACTOB2B,&#xD;&#xA; &#xD;&#xA; (select&#xD;&#xA;  this_.NOMBREUSUARIO&#xD;&#xA; from&#xD;&#xA;    smxcorp.SSPCOVUSUFUNPERLOC this_ &#xD;&#xA; inner join&#xD;&#xA;    KSEMPTUSECOMSYS sistemasus1_ &#xD;&#xA;    on this_.codigoCompania=sistemasus1_.COMID &#xD;&#xA;    and this_.codigoUsuario=sistemasus1_.USERID &#xD;&#xA; inner join smxmbase.KSSEGTROLUSE ur&#xD;&#xA; on this_.codigousuario = ur.userid&#xD;&#xA; where&#xD;&#xA;    this_.codigoCompania=1 and this_.codigoTipoRelacionFuncionario=141 and this_.tipoRelacionFuncionario like 'PRO' &#xD;&#xA;    and this_.codigoEntidad=P.CODIGOENTIDADPROVEEDOR and this_.valorTipoEntidad = P.VALORTIPOENTIDADPROVEEDOR and substr(this_.codigousuario,1,9) &lt;> 'B2BPROADM'&#xD;&#xA;    and sistemasus1_.SYSID='B2B' and sistemasus1_.userCompanySystemStatus='1' and this_.estadousuariopersonalocalizacion = 'ACT'&#xD;&#xA;    and ur.userrolstatus = '1' and ur.roleid = 'B2BPED' order by ur.userid FETCH FIRST 1 ROWS ONLY) AS NOMBRECONTACTOPED,&#xD;&#xA;   &#xD;&#xA; (select&#xD;&#xA;  this_.EMAILUSUARIO&#xD;&#xA; from&#xD;&#xA;    smxcorp.SSPCOVUSUFUNPERLOC this_ &#xD;&#xA; inner join&#xD;&#xA;    KSEMPTUSECOMSYS sistemasus1_ &#xD;&#xA;    on this_.codigoCompania=sistemasus1_.COMID &#xD;&#xA;    and this_.codigoUsuario=sistemasus1_.USERID &#xD;&#xA; inner join smxmbase.KSSEGTROLUSE ur&#xD;&#xA; on this_.codigousuario = ur.userid&#xD;&#xA; where&#xD;&#xA;    this_.codigoCompania=1 and this_.codigoTipoRelacionFuncionario=141 and this_.tipoRelacionFuncionario like 'PRO' &#xD;&#xA;    and this_.codigoEntidad=P.CODIGOENTIDADPROVEEDOR and this_.valorTipoEntidad = P.VALORTIPOENTIDADPROVEEDOR and substr(this_.codigousuario,1,9) &lt;> 'B2BPROADM'&#xD;&#xA;    and sistemasus1_.SYSID='B2B' and sistemasus1_.userCompanySystemStatus='1' and this_.estadousuariopersonalocalizacion = 'ACT'&#xD;&#xA;    and ur.userrolstatus = '1' and ur.roleid = 'B2BPED' order by ur.userid FETCH FIRST 1 ROWS ONLY) AS MAILCONTACTOPED,&#xD;&#xA;   &#xD;&#xA; (select&#xD;&#xA;  TRIM(REPLACE(REPLACE(REPLACE(this_.NOMBREUSUARIO,'ADMINISTRADOR',''),'(',''),')',''))  &#xD;&#xA; from&#xD;&#xA;    smxcorp.SSPCOVUSUFUNPERLOC this_ &#xD;&#xA; inner join&#xD;&#xA;    KSEMPTUSECOMSYS sistemasus1_ &#xD;&#xA;    on this_.codigoCompania=sistemasus1_.COMID &#xD;&#xA;    and this_.codigoUsuario=sistemasus1_.USERID &#xD;&#xA; inner join smxmbase.KSSEGTROLUSE ur&#xD;&#xA; on this_.codigousuario = ur.userid&#xD;&#xA; where&#xD;&#xA;    this_.codigoCompania=1 and this_.codigoTipoRelacionFuncionario=141 and this_.tipoRelacionFuncionario like 'PRO' &#xD;&#xA;    and this_.codigoEntidad=P.CODIGOENTIDADPROVEEDOR and this_.valorTipoEntidad = P.VALORTIPOENTIDADPROVEEDOR&#xD;&#xA;    and sistemasus1_.SYSID='B2B' and sistemasus1_.userCompanySystemStatus='1' and this_.estadousuariopersonalocalizacion = 'ACT'&#xD;&#xA;    and ur.userrolstatus = '1' and ur.roleid = 'B2BPROADM' FETCH FIRST 1 ROWS ONLY) AS NOMBRECONTACTOADM,&#xD;&#xA;&#x9;P.RAZONSOCIALPROVEEDOR,&#xD;&#xA;&#x9;P.FECHACREACION,&#xD;&#xA;&#x9;P.ESTADOPROVEEDOR,&#xD;&#xA;&#x9;VALORTIPOEMPRESA ABR_TIPO_EMPRESA,&#xD;&#xA;&#x9;TIPOEMPRESA.NOMBRECATALOGOVALOR NOM_TIPO_EMPRESA,&#xD;&#xA;&#x9;EPS.NOMBRECATALOGOVALOR ABR_CALIFICACION_EMPRESA,&#xD;&#xA;&#x9;'ND'  NOM_CALIFICACION_EMPRESA,&#xD;&#xA;&#x9;ARTESANO.NOMBRECATALOGOVALOR ESARTESANO,&#xD;&#xA;&#x9;ORIGENPROVEEDOR, &#xD;&#xA;&#x9;PROIMP.ESIMPORTADOR,&#xD;&#xA;&#x9;PLAZOP.NOMBRECATALOGOVALOR PLAZO_PAGO,&#xD;&#xA;&#x9;PLAZOP.VALORNUMERICO VALOR_PLAZO_PAGO,&#xD;&#xA;&#x9;CIUDAD.NOMBREDIVGEOPOL CIUDAD,&#xD;&#xA;&#x9;CASE WHEN A.CODIGOPROVEEDOR IS NULL THEN 0 ELSE 1 END ANALITIX&#xD;&#xA;FROM SMXSIC.SCSPETPROVEEDOR P&#xD;&#xA;LEFT JOIN SMXSIC.SSPCOTCATALOGOVALOR TIPOEMPRESA &#xD;&#xA;ON P.CODIGOTIPOEMPRESA = TIPOEMPRESA.CODIGOCATALOGOTIPO &#xD;&#xA;AND P.VALORTIPOEMPRESA = TIPOEMPRESA.CODIGOCATALOGOVALOR&#xD;&#xA;LEFT JOIN SMXSIC.SCSADTPROIMP PROIMP &#xD;&#xA;ON P.CODIGOCOMPANIA = PROIMP.CODIGOCOMPANIA &#xD;&#xA;AND P.CODIGOPROVEEDOR = PROIMP.CODIGOPROVEEDOR&#xD;&#xA;LEFT JOIN SMXSIC.SCSADTPROCOM PROCOM &#xD;&#xA;ON P.CODIGOCOMPANIA = PROCOM.CODIGOCOMPANIA &#xD;&#xA;AND P.CODIGOPROVEEDOR = PROCOM.CODIGOPROVEEDOR&#xD;&#xA;LEFT JOIN SMXSIC.SSPCOTCATALOGOVALOR PLAZOP &#xD;&#xA;ON PROCOM.CODIGOTIPOPLAZOPAGO = PLAZOP.CODIGOCATALOGOTIPO &#xD;&#xA;AND PROCOM.VALORTIPOPLAZOPAGO = PLAZOP.CODIGOCATALOGOVALOR&#xD;&#xA;LEFT JOIN SMXSIC.SCPROTCARPRO CARPRO &#xD;&#xA;ON P.CODIGOCOMPANIA = CARPRO.CODIGOCOMPANIA &#xD;&#xA;AND P.CODIGOPROVEEDOR = CARPRO.CODIGOPROVEEDOR &#xD;&#xA;AND CARPRO.CODIGOTIPOCARPRO =100565&#xD;&#xA;&#xD;&#xA;LEFT JOIN SMXSIC.SCPROTCARPRO CARPROEPS &#xD;&#xA;ON P.CODIGOCOMPANIA = CARPROEPS.CODIGOCOMPANIA &#xD;&#xA;AND P.CODIGOPROVEEDOR = CARPROEPS.CODIGOPROVEEDOR &#xD;&#xA;AND CARPROEPS.CODIGOTIPOCARPRO =100451&#xD;&#xA;LEFT JOIN SMXSIC.SSPCOTCATALOGOVALOR EPS &#xD;&#xA;ON CARPROEPS.CODIGOTIPOCARPRO = EPS.CODIGOCATALOGOTIPO &#xD;&#xA;AND CARPROEPS.CODIGOVALORCARPRO = EPS.CODIGOCATALOGOVALOR&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;LEFT JOIN SMXSIC.SSPCOTCATALOGOVALOR ARTESANO &#xD;&#xA;ON CARPRO.CODIGOTIPOCARPRO = ARTESANO.CODIGOCATALOGOTIPO &#xD;&#xA;AND CARPRO.CODIGOVALORCARPRO = ARTESANO.CODIGOCATALOGOVALOR&#xD;&#xA;LEFT JOIN SMXSIC.SSPCOTDATCONPERLOC CONPER &#xD;&#xA;ON P.CODIGOCOMPANIA = CONPER.CODIGOCOMPANIA &#xD;&#xA;AND P.CODIGOPERSONA = CONPER.CODIGOPERSONA &#xD;&#xA;AND CODIGOTIPOCONTACTO = 'PRI' AND CONPER.ESTADO='ACT'&#x9;&#xD;&#xA;LEFT JOIN SMXSIC.SSPCOTDIVISIONGEOPOLITICA CIUDAD &#xD;&#xA;ON CONPER.CODCIUDIRPRI = CIUDAD.CODIGODIVGEOPOL&#xD;&#xA;LEFT JOIN SMXSIC.SCRBDTPROVACS A&#xD;&#xA;ON A.CODIGOPROVEEDOR = P.CODIGOPROVEEDOR AND A.ESTADO = '1'&#xD;&#xA;&#xD;&#xA;) AS TMP ORDER BY TMP.CODIGOJDEPROVEEDOR&quot;"/>
